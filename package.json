{
  "name": "moov2-orchard-core-gallery",
  "version": "1.0.0",
  "description": "Module for [Orchard Core](https://github.com/OrchardCMS/OrchardCore) for displaying a collection of images & videos.",
  "main": "index.js",
  "author": "Moov2, Etch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moov2/Moov2.OrchardCore.Gallery/issues"
  },
  "homepage": "https://github.com/moov2/Moov2.OrchardCore.Gallery#readme",
  "dependencies": {
    "lg-video.js": "^1.0.0",
    "lightgallery.js": "^1.0.2",
    "browserify": "^14.4.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0"
  },
  "scripts": {
    "develop": "npm run js && npm run css",
    "css": "node-sass --include-path scss ./assets/styles/styles.scss ./wwwroot/css/styles.css",
    "js": "mkdirp ./wwwroot/js && cross-env NODE_PATH=./node_modules/lightgallery.js/src/js:./node_modules/lg-video.js/dist browserify ./assets/scripts/gallery.js > ./wwwroot/js/bundle.js -t [babelify --presets [es2015] ]"
  }
}
