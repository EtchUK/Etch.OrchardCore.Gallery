{
  "name": "moov2-orchard-core-gallery",
  "version": "1.0.0",
  "description": "Module for [Orchard Core](https://github.com/OrchardCMS/OrchardCore) for displaying a collection of images & videos.",
  "main": "index.js",
  "author": "Moov2, Etch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moov2/Moov2.OrchardCore.Gallery/issues"
  },
  "homepage": "https://github.com/moov2/Moov2.OrchardCore.Gallery#readme",
  "dependencies": {
    "lg-video.js": "^1.0.0",
    "lightgallery.js": "^1.0.2",
    "browserify": "^14.4.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0"
  },
  "devDependencies": {
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "build:admin": "webpack",
    "build:site": "npm run js:site && npm run css:site",
    "css:site": "node-sass --include-path scss ./assets/styles/styles.scss ./wwwroot/css/styles.css",
    "js:site": "mkdirp ./wwwroot/js && cross-env NODE_PATH=./node_modules/lightgallery.js/src/js:./node_modules/lg-video.js/dist browserify ./assets/js/display/gallery.js > ./wwwroot/Scripts/bundle.js -t [babelify --presets [es2015] ]"
  }
}
